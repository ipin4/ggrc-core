    {{/widget_list}}

    {{#is_allowed 'update' instance}}
    {{#if widget_list.length}}
    {{^if instance.archived}}
    {{#isMenuVisible}}
    <li data-test-id="button_widget_add_2c925d94" class="hidden-widgets-list">
