                    {{/if_equals}}
                    {{/is_allowed_to_map}}
                    {{/is_mappable_type}}
                    {{/is_dashboard_or_all}}
                  >
                    <div class="oneline">
                      <i class="fa fa-{{ internav_icon }} color"></i>
                      {{internav_display}}
                    </div>
                  </a>
                {{/if_helpers}}

              {{/if_helpers}}
              {{/isNotProhibitedMap}}
            {{/isNotObjectVersion}}
          </div>
        {{/widget_list}}
      </div>
    </li>
    {{/isMenuVisible}}
    {{/if}}
    {{/if}}
    {{/is_allowed}}
  </ul>
</div>
{{/if_equals}}
{{/if}}
